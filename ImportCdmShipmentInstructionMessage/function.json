{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "$return",
      "databaseName": "Kairos",
      "collectionName": "messages",
      "createIfNotExists": true,
      "connectionStringSetting": "unicorn-peter-cosmos_DOCUMENTDB",
      "direction": "out"
    }
  ]
}
